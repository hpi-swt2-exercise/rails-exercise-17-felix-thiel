<p>
    <strong>Title:</strong>
    <%= @paper.title %>
</p>
<p>
    <strong>Venue:</strong>
    <%= @paper.venue %>
</p>
<p>
    <strong>Year:</strong>
    <%= @paper.year %>
</p>

<p>
    <strong>Authors:</strong>
    <% @paper.authors.each do |author| %>
        <br>
        <% if !author.nil? %>
            <%= author.name %>
        <% end %>
    <% end %>
</p>

<%= link_to 'Back', papers_path %>
